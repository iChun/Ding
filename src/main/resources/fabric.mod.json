{
  "schemaVersion": 1,
  "id": "ding",
  "version": "${version}",

  "name": "Ding",
  "description": "Plays a sound once the client has loaded to the main menu, a server, or resources",
  "authors": [
    "iChun"
  ],
  "contact": {
    "homepage": "https://github.com/iChun/Ding",
    "issues": "https://github.com/iChun/Ding/issues"
  },

  "license": "GNU Lesser General Public License v3.0",
  "icon": "icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "me.ichun.mods.ding.loader.fabric.LoaderFabricClient"
    ],
    "modmenu": [
      "me.ichun.mods.ding.loader.fabric.ModMenuIntegration"
    ]
  },

  "mixins": [
    "ding.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.15.0",
    "fabric-api": "*",
    "minecraft": ">=1.20.4 <1.21",
    "java": ">=17",
    "completeconfig-base": ">=2.5.2"
  }
}
